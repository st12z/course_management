mixin alert-success(time)
  - if (messages.success)
    .message.info
      div(class="alert alert-success"
        data-time=time
        show-alert
      ) #{messages.success} 
mixin alert-error(time)
  - if (messages.error)
    .message.info
      div(class="alert alert-error"
        show-alert
      ) #{messages.error} 